@model IEnumerable<string>

@Html.ActionLink("Home", "List", "Posts")


@foreach (var link in Model)
{
    @Html.RouteLink(link, new {controller = "Posts",
      action = "List",
      category = link,
      page = 1},
      
      new
      {
          @class = link == ViewBag.SelectedCategory ? "selected" : null
      })
}